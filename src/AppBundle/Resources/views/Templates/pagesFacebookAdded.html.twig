<table id="tabResultAddedPage" class="tableau">
    <thead>
    <tr>
        <th>Name</th>
        <th>Page</th>
        <th>Options</th>
    </tr>
    </thead>
    <tbody>
    {% if tabResult is not empty %}
        {% for line in tabResult %}
            <tr data-id={{ line.id }}>
                <td>{{ line.name }}</td>
                <td><a href="https://www.facebook.com/{{ line.id }}" target="_blank">{{ line.name }}</a></td>
                <td>
                    <div class="suppr-page-fade btn red-btn" data-id={{ line.id }} data-name='{{ line.name }}'>Delete</div>
                </td>
            </tr>
        {% endfor %}
    {% else %}
        <tr>
            <td colspan=3>No pages added</td>
        </tr>
    {% endif %}
    </tbody>
</table>