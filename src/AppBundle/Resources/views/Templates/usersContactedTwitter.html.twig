<table id="tabResultUser" class="tableau">
    <thead>
    <tr>
        <th>Nom</th>
        <th>Tweet</th>
        <th>Options</th>
    </tr>
    </thead>
    <tbody>
    {% if tabResult is not empty %}
        {% for user in tabResult %}
            <tr>
                <td>{{ user.name }}</td>
                <td class="width20-text padding-10">{{ user.info }}</td>
                <td>
                    <a href="https://www.twitter.com/{{ user.name }}" target="_blank">
                        <div class="see-profil-btn btn green-btn margin-10" data-id={{ user.id }}>Voir profil</div>
                    </a>
                </td>
            </tr>
        {% endfor %}
    {% else %}
        <tr>
            <td colspan=3>Aucun utilisateur ajout√©</td>
        </tr>
    {% endif %}
    </tbody>
</table>